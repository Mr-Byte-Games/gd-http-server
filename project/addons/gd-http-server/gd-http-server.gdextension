[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.2
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://lib/linux/debug/libgd_http_server.so"
linux.release.x86_64 =   "res://lib/linux/release/libgd_http_server.so"
windows.debug.x86_64 =   "res://lib/windows/debug/gd_http_server.dll"
windows.release.x86_64 = "res://lib/windows/release/gd_http_server.dll"
macos.debug =            "res://lib/macos/debug/libgd_http_server.dylib"
macos.release =          "res://lib/macos/release/libgd_http_server.dylib"
macos.debug.arm64 =      "res://lib/macos/debug/libgd_http_server.dylib"
macos.release.arm64 =    "res://lib/macos/release/libgd_http_server.dylib"